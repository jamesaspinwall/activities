<p id="notice"><%= notice %></p>

<h1>Upworks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Country</th>
      <th>Skill</th>
      <th>Rate</th>
      <th>Hours</th>
      <th>Jobs</th>
      <th>Top</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upworks.each do |upwork| %>
      <tr>
        <td><%= upwork.name %></td>
        <td><%= upwork.country %></td>
        <td><%= upwork.skill %></td>
        <td><%= upwork.rate %></td>
        <td><%= upwork.hours %></td>
        <td><%= upwork.jobs %></td>
        <td><%= upwork.top %></td>
        <td><%= link_to 'Show', upwork %></td>
        <td><%= link_to 'Edit', edit_upwork_path(upwork) %></td>
        <td><%= link_to 'Destroy', upwork, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upwork', new_upwork_path %>
